<p id="notice"><%= notice %></p>

<h1>Teachers Cohorts</h1>

<table>
  <thead>
    <tr>
      <th>Teacher id</th>
      <th>Student id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teachers_cohorts.each do |teachers_cohort| %>
      <tr>
        <td><%= teachers_cohort.Teacher_ID %></td>
        <td><%= teachers_cohort.Student_ID %></td>
        <td><%= link_to 'Show', teachers_cohort %></td>
        <td><%= link_to 'Edit', edit_teachers_cohort_path(teachers_cohort) %></td>
        <td><%= link_to 'Destroy', teachers_cohort, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teachers Cohort', new_teachers_cohort_path %>
